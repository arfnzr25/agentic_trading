# ===== HYPERLIQUID CREDENTIALS =====
# Get these from Hyperliquid Arbitrum Testnet or Mainnet
HL_PK=your_private_key_here
HL_WL=your_wallet_address_here

# ===== MCP SERVER CONFIG =====
# URL of the MCP server (Stdio or SSE)
MCP_SERVER_URL=http://localhost:8000/sse

# ===== LLM API KEYS =====
# Required for Analyst and Shadow Agent
OPENROUTER_API_KEY=your_openrouter_key
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
# OR
OPENAI_API_KEY=your_openai_key

# ===== AGENT CONFIG =====
ANALYST_MODEL=deepseek/deepseek-chat
RISK_MODEL=deepseek/deepseek-chat
INFERENCE_INTERVAL=180
USE_V2_ANALYST=1
USE_V2_RISK=1
FOCUS_COINS=["BTC"]

# ===== NOTIFICATIONS (TELEGRAM) =====
# 1. Start a chat with @BotFather to get a Token
# 2. Start a chat with @userinfobot to get your Chat ID
TELEGRAM_BOT_TOKEN=your_bot_token
TELEGRAM_CHAT_ID=your_chat_id
